<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
   http://www.springframework.org/schema/beans     
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context 
   http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="student" class="com.spring.model.Student" lazy-init="false" parent="person">
		<property name="studentId" value="27"></property>
		<property name="studentName" value="Keyur"></property>
		<property name="phone" value="7387029671"></property>
		<property name="city" value="Surat"></property>

	</bean>

	<bean id="person" class="com.spring.model.Person">
		<property name="email" value="keyur.555kn@gmail.com"></property>
		<property name="gender" value="Male"></property>
		<property name="city" value="Pune"></property>
	</bean>

</beans>